{
  "manifest_version": 2,

  "name": "GiftForYou",
  "description": "Get Innterest Someone",
  "version": "1.0",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "GiftForYou",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "css": [],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs","activeTab","storage"]
}
