{
  "manifest_version": 2,

  "name": "HackChrome",
  "description": "Wrapping info profile",
  "version": "1.0",
  "icons" :{
    "75" : "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "HackChrome",
    "default_popup": "popup.html"
  },
  "background":{
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["thirdparty/jquery-3.2.1.min.js","myScript.js"]
    }
  ],
  "permissions": ["tabs","activeTab","storage"]
}
