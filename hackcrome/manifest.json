{
  "manifest_version": 2,

  "name": "HackChrome",
  "description": "Wrapping info profile",
  "version": "1.0",
  "icons" :{
    "75" : "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "HackChrome",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["myScript.js"]
    }
  ],
  "permissions": ["tabs","activeTab"]
}
